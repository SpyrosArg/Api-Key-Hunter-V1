{
  "name": "apikeyhunter",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --force",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "cross-env NODE_ENV=production node server.js",
    "dev:server": "cross-env NODE_ENV=development node server.js",
    "dev:all": "concurrently -k \"npm run dev:server\" \"npm run dev\" --prefix-colors \"blue.bold,green.bold\" --names \"server,vite\"",
    "clean": "rm -rf .vite-cache node_modules/.vite dist"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "cheerio": "1.0.0-rc.12",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.18.3",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.4"
  }
}
